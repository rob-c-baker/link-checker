{
  "name": "alan-rogers/link-cheker",
  "description": "Crawl supplied website and reports any broken links.",
  "license": "proprietary",
  "type": "project",
  "authors": [
    {
      "name": "Rob Baker",
      "email": "rob.baker@alanrogers.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "developers@alanrogers.com",
    "issues": "https://gitlab.alanrogers.com/web-dev/worldwide/-/issues",
    "source": "https://gitlab.alanrogers.com/web-dev/worldwide",
    "docs": "https://gitlab.alanrogers.com/web-dev/worldwide/-/wikis/home"
  },
  "require": {
    "php": "^8.3",
    "sentry/sentry": "^4.7",
    "symfony/console": "^7.0",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "alanrogers\\linkchecker\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform": {
      "php": "8.3"
    }
  },
  "bin": [
    "bin/link-checker"
  ],
  "scripts": {},
  "require-dev": {
    "symfony/var-dumper": "^7.0"
  }
}
